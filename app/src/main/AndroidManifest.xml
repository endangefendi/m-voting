<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fend.m_voting">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/hasil"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Hasil.Hasil" />
        <activity android:name=".Jadwal.AturJadwalRegistrasi" />
        <activity android:name=".Pemilih.TampilQRCodePemilih" />
        <activity android:name=".Vote.Scan" />
        <activity android:name=".Admin.TambahAdmin" />
        <activity android:name=".Admin.LihatAdmin" />
        <activity android:name=".Hasil.DetailHasil" />
        <activity android:name=".Hasil.HasilVote" />
        <activity android:name=".Calon.UACalon" />
        <activity android:name=".Calon.DetailCalon" />
        <activity android:name=".Calon.LihatCalon" />
        <activity android:name=".Calon.TambahCalon" />
        <activity android:name=".Pemilih.UAPemilih" />
        <activity android:name=".Pemilih.DetailPemilih" />
        <activity android:name=".Pemilih.TampilPemilih" />
        <activity android:name=".Pemilih.Daftar" />
        <activity android:name=".Calon.CalonActivity" />
        <activity android:name=".Pemilih.PemilihActivity" />
        <activity android:name=".Admin.AdminActivity" />
        <activity android:name=".Jadwal.AturPengumuman" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".Service.MyFirebaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>